formulas:
  Untitled: ""
views:
  - type: cards
    name: Table
    filters:
      or:
        - Слушаю == false
        - Слушаю == true
    order:
      - file.name
      - Слушаю
      - Прослушано
      - Прогресс
      - ПрогрессMax
    imageFit: ""
    image: note.Обложка
    imageAspectRatio: 1.5
  - type: table
    name: Навыки
    filters:
      and:
        - file.name == "Полезные Сайты Useful Sites"
    order:
      - file.name
      - file.tags
      - formula.Untitled
    columnSize:
      file.name: 300
  - type: table
    name: представление
  - type: table
    name: К прочтению
  - type: table
    name: Прочитано
  - type: table
    name: К прослушиванию
    filters:
      and:
        - file.folder.contains("Контент/Литература")
        - and:
            - Читаю == true
            - Слушаю == true
        - and:
            - Прочитано == true
            - Прочитано == true
    order:
      - file.name
  - type: table
    name: Читаю
  - type: table
    name: Слушаю
  - type: table
    name: Прослушано
